<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All attendences
    </h1>

    <a href="/attendences/new" class="btn btn-block btn-outline-secondary">
      Add a new attendence
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          User
        </th>

        <th>
          Restaurant
        </th>

        <th>
          Bar
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @attendences.each do |attendence| %>
      <tr>
        <!-- Display attendence.id start -->
        <td>
          <%= attendence.id %>
        </td>
        <!-- Display attendence.id end -->

        <!-- Display attendence.user_id start -->
        <td>
          <%= attendence.user_id %>
        </td>
        <!-- Display attendence.user_id end -->

        <!-- Display attendence.restaurant_id start -->
        <td>
          <%= attendence.restaurant_id %>
        </td>
        <!-- Display attendence.restaurant_id end -->

        <!-- Display attendence.bar_id start -->
        <td>
          <%= attendence.bar_id %>
        </td>
        <!-- Display attendence.bar_id end -->

        <!-- Display attendence.created_at start -->
        <td>
          <%= time_ago_in_words(attendence.created_at) %> ago
        </td>
        <!-- Display attendence.created_at end -->

        <!-- Display attendence.updated_at start -->
        <td>
          <%= time_ago_in_words(attendence.updated_at) %> ago
        </td>
        <!-- Display attendence.updated_at end -->

        <td>
          <a href="/attendences/<%= attendence.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
